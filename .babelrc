{
  "comments": false,
  "env": {
    "main": {
      "presets": [
        ["env", {
          "targets": { "node": 7 },
          "modules": false
        }],
        "stage-0"
      ]
    },
    "renderer": {
      "presets": [
        "@babel/preset-env"
      ]
    },
    "web": {
      "presets": [
        "@babel/preset-env"
      ]
    }
  },
  "plugins": ["@babel/plugin-transform-runtime"]
}
